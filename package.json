{
  "name": "build-station-3d",
  "version": "1.0.0",
  "description": "Build-Station 3D - Interactive Room Configurator",
  "scripts": {
    "start": "http-server public -p 3000 -c-1 --cors",
    "dev": "npm start",
    "serve": "http-server public -p 3000 -c-1 --cors -o",
    "build": "echo 'Already built - production bundle in dist/'",
    "analyze": "source-map-explorer 'dist/js/*.bundle.js' --html bundle-report.html",
    "deploy": "vercel --prod",
    "preview": "vercel",
    "test": "echo 'Deploy to Vercel: npm run deploy'",
    "check-assets": "find public/assets -type f | wc -l"
  },
  "keywords": [
    "3d",
    "room-configurator",
    "floor-planner",
    "threejs",
    "interior-design"
  ],
  "author": "Build-Station",
  "license": "MIT",
  "devDependencies": {
    "http-server": "^14.1.1",
    "js-beautify": "^1.14.11",
    "playwright": "^1.57.0",
    "prettier": "^3.1.0",
    "source-map-explorer": "^2.5.3",
    "webpack-bundle-analyzer": "^4.10.1"
  }
}
